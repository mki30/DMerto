{
  "name": "Delhi Metro",
  "version": "1.0",
   "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["jquery-1.7.1.min.js", "myscript.js"]
    }
  ],
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Set this page's color.",
      "default_icon": "metro.ico",
      "popup": "post.html"
  }
}